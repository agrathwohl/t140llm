{
  "extends": "@reallyland/tsconfig",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "declarationDir": "dist",
    "module": "CommonJS",
    "skipLibCheck": true,
    "importsNotUsedAsValues": "remove"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["dist", "node_modules"]
}
